<?xml version="1.0" encoding="ISO-8859-1" ?>
<?xml-stylesheet type="text/xsl" href="xsl/mdb2_changeset.xsl"?>
<instructionset>
  <name>openads</name>
  <version>998</version>
  <comments></comments>
  <constructive>
    <changeset>
      <name>openads</name>
      <version>998</version>
      <add>
        <table>
        <name>aardvark</name>
        <was>aardvark</was>
      </table>
    </add>
    <change>
      <table>
        <name>acls_channel</name>
        <add>
          <field>
            <name>channel_id</name>
            <type>openads_mediumint</type>
            <length>9</length>
            <notnull>1</notnull>
            <default>0</default>
            <was>channelid</was>
          </field>
            <field>
              <name>newfield</name>
              <type>openads_datetime</type>
              <was>newfield</was>
            </field>
          </add>
          <index>
            <remove>
              <name>channelid</name>
            </remove>
            <remove>
              <name>channelid_executionorder</name>
            </remove>
            <add>
              <name>channelid</name>
              <was>channelid</was>
              <indexfield>
                <name>channel_id</name>
                <sorting>ascending</sorting>
              </indexfield>
            </add>
              <add>
                <name>channelid_executionorder</name>
                <was>channelid_executionorder</was>
                <unique>true</unique>
                <indexfield>
                  <name>channel_id</name>
                  <sorting>ascending</sorting>
                </indexfield>
                <indexfield>
                  <name>executionorder</name>
                  <sorting>ascending</sorting>
                </indexfield>
              </add>
            </index>
          </table>
        </change>
      </changeset>
    </constructive>
    <destructive>
      <changeset>
        <name>openads</name>
        <version>998</version>
        <change>
          <table>
            <name>acls_channel</name>
          <remove>
            <field>
              <name>channelid</name>
            </field>
          </remove>
        </table>
      </change>
    </changeset>
  </destructive>
</instructionset>
